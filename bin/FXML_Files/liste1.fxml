            <TableColumn fx:id="cin" prefWidth="75.0" text="cin" />
            <TableColumn fx:id="numtel" prefWidth="75.0" text="num_tel" />
            <TableColumn fx:id="adresse" prefWidth="75.0" text="adresse" />
            <TableColumn fx:id="e_mail" prefWidth="75.0" text="e_mail" />
            <TableColumn fx:id="description" prefWidth="75.0" text="description" />
            <TableColumn fx:id="date" prefWidth="75.0" text="date" />
        </columns>
      </TableView>
      <TextField layoutX="41.0" layoutY="22.0" onAction="#rechercher" promptText="rechercher" />
      <Button layoutX="23.0" layoutY="336.0" mnemonicParsing="false" onAction="#ajouter" text="ajouter" />
      <Button layoutX="126.0" layoutY="336.0" mnemonicParsing="false" onAction="#supprimer" text="supprimer" />
      <Button layoutX="239.0" layoutY="336.0" mnemonicParsing="false" onAction="#modifier" text="modifier" />
